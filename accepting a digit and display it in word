#include<iostream>
using namespace std;
int main(){
	int digit;
	cout<<"enter a digit"<<endl;
	cin>>digit;
	if (digit==0)
	cout<<"zero";
	else if(digit==1)
	cout<<"one";
	else if (digit==2)
	cout<<"two";
	else if(digit==3)
	cout<<"three";
	else if(digit==4)
	cout<<"four";
	else if(digit==5)
	cout<<"five";
	else if (digit==6)
	cout<<"six";
	else
	cout<<"not a digit";
}
