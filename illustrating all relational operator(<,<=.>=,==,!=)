#include<iostream>
using namespace std;

int main(){
    int a,b,c,d,e,f,g,h;
    cout<<"enter two number="<<endl;
    cin>>a>>b;
    cout<<"a= "<<a<<"b= "<<b<<endl;
    c=a<b;
    d=a<=b;
    e=a>b;
    f=a>=b;
    g=a==b;
    h=a!=b;
    cout<<c<<endl<<d<<endl<<e<<endl<<f<<endl<<g<<endl<<h<<endl;
}
